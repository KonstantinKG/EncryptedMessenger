{
  "app": "encrypted-chat-ws",
  "host": "0.0.0.0",
  "port": 8201,
  "logger": {
      "version": 1,
      "disable_existing_loggers": true,
      "formatters": {
          "console": {
              "format": "%(asctime)s — [ %(levelname)s ] — %(message)s"
          }
      },
      "handlers": {
          "console": {
              "class": "logging.StreamHandler",
              "stream": "ext://sys.stdout",
              "formatter": "console"
          }
      },
      "loggers": {
          "": {
              "handlers": [
                "console"
              ],
              "level": "INFO",
              "propagate": true
          }
      }
  }
}
